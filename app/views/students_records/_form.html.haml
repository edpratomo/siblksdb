= form_for @students_record do |f|
  - if @students_record.errors.any?
    #error_explanation
      %h2= "#{pluralize(@students_record.errors.count, "error")} prohibited this students_record from being saved:"
      %ul
        - @students_record.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :student_id
    = f.number_field :student_id
  .field
    = f.label :pkg_id
    = f.number_field :pkg_id
  .field
    = f.label :started_on
    = f.datetime_select :started_on
  .field
    = f.label :finished_on
    = f.datetime_select :finished_on
  .field
    = f.label :status
    = f.text_area :status
  .field
    = f.label :modified_at
    = f.datetime_select :modified_at
  .field
    = f.label :modified_by
    = f.text_area :modified_by
  .actions
    = f.submit 'Save'
